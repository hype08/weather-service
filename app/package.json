{
  "name": "weather-service",
  "description": "A weather forecast app built on Next.js and Open Weather",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "date-fns": "^2.20.2",
    "lint-staged": "^10.5.4",
    "next": "latest",
    "react": "^17.0.1",
    "react-cool-onclickoutside": "^1.6.1",
    "react-dom": "^17.0.1",
    "swr": "^0.5.5",
    "use-places-autocomplete": "^1.9.1"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.43.3",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "postcss": "^8.2.9",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.1",
    "typescript": "4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "keywords": [
    "weather",
    "forecast"
  ],
  "homepage": "https://weather-service.vercel.app/",
  "author": "Henry <henry.zhang@hey.com>",
  "license": "MIT",
  "bugs": "https://github.com/hype08/weather-service/issues"
}
